{
  "name": "cm-nextjs-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prod": "yarn decrypt-production-env && yarn dev",
    "dev:staging": "yarn decrypt-staging-env && yarn dev",
    "build": "next build",
    "build:prod": "yarn decrypt-production-env && next build",
    "build:staging": "yarn decrypt-staging-env && next build",
    "start": "next start",
    "lint": "next lint",
    "encrypt-production-env": "openssl aes-256-cbc -in .env.local -out keys/production.enc -pass file:.encryption.key -md sha512 -pbkdf2",
    "decrypt-production-env": "openssl aes-256-cbc -d -in keys/production.enc -out .env.local -pass file:.encryption.key -md sha512 -pbkdf2",
    "encrypt-staging-env": "openssl aes-256-cbc -in .env.local -out keys/staging.enc -pass file:.encryption.key -md sha512 -pbkdf2",
    "decrypt-staging-env": "openssl aes-256-cbc -d -in keys/staging.enc -out .env.local -pass file:.encryption.key -md sha512 -pbkdf2",
    "encrypt-dev-env": "openssl aes-256-cbc -in .env.local -out keys/development.enc -pass file:.encryption.key -md sha512 -pbkdf2",
    "decrypt-dev-env": "openssl aes-256-cbc -d -in keys/development.enc -out .env.local -pass file:.encryption.key -md sha512 -pbkdf2",
    "test-all": "yarn lint && yarn build:prod --debug",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "autoprefixer": "^10.4.7",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "sass": "^1.52.1",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
