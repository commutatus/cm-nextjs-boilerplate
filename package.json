{
  "name": "cm-nextjs-boilerplate",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prod": "yarn decrypt-production-env && yarn dev",
    "dev:staging": "yarn decrypt-staging-env && yarn dev",
    "build": "next build",
    "build:prod": "yarn decrypt-production-env && next build",
    "build:staging": "yarn decrypt-staging-env && next build",
    "start": "next start",
    "lint": "next lint",
    "encrypt-production-env": "cm-env encrypt prod",
    "decrypt-production-env": "cm-env decrypt prod",
    "encrypt-staging-env": "cm-env encrypt staging",
    "decrypt-staging-env": "cm-env decrypt staging",
    "encrypt-dev-env": "cm-env encrypt dev",
    "decrypt-dev-env": "cm-env decrypt dev",
    "create-encryption-keys": "cm-env setup prod && cm-env setup staging && cm-env setup dev",
    "test-all": "yarn lint && yarn build:staging --debug",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@commutatus/cm-env": "^1.1.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "autoprefixer": "^10.4.7",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "sass": "^1.52.1",
    "tailwindcss": "^3.0.24",
    "typescript": "4.6.4"
  }
}
